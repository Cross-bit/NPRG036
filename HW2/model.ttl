# Časový slot

ex:TimeSlot a rdfs:Class ;
	rdfs:label "Časový slot"@cs ;
	rdfs:comment "Reprezentuje časové okno od-do, v němž probíhá výuka."@cs ;
	rdfs:subClassOf time:GeneralDateTimeDescription .

# [Začátek časového slotu]
ex:(TimeSlotBeginning) a rdf:Property ;
	rdfs:label "Začátek časového slotu"@cs ;
	rdfs:comment "Čas, kdy začíná časový slot."@cs ;
	rdfs:domain ex:TimeSlot ;
	rdfs:range xsd:time .

# [Konec časového slotu]
ex:(TimeSlotEnd) a rdf:Property ;
	rdfs:label "Konec časového slotu"@cs ;
	rdfs:comment "Čas, kdy končí časový slot."@cs ;
	rdfs:domain ex:TimeSlot ;
	rdfs:range xsd:time .

# [Den v týdnu]
ex:DayOfWeek a rdf:Property ;
	rdfs:label "Den v týdnu"@cs ;
	rdfs:comment "Den v týdnu, který se časový slot odehrává."@cs ;
	rdfs:domain ex:TimeSlot ;
	rdfs:range xsd:dayOfWeek .

# 	Vlastnost: den_v_týdnu
#	time:dayOfWeek time:DayOfWeek (time:Monday/time:Tuesday/time:Wednesday/time:Thursday/time:Friday/time:Saturday/time:Sunday)
	


@prefix exTimeSlot: <http://example.org/data/timeslots> . 

exTimeSlot:Timeslot1 a ex:TimeSlot ;
	ex:TimeSlotBeginning "08:00:00"^^xsd:time ;
	ex:TimeSlotEnd "08:45:00"^^xsd:time ;
	ex:DayOfWeek "Monday"^^time:dayOfWeek .

exTimeSlot:Timeslot2 a ex:TimeSlot ;
	ex:TimeSlotBeginning "08:55:00"^^xsd:time ;
	ex:TimeSlotEnd "09:40:00"^^xsd:time ;
	ex:DayOfWeek "Thursday"^^time:dayOfWeek .

exTimeSlot:Timeslot3 a ex:TimeSlot ;
	ex:TimeSlotBeginning "10:00:00"^^xsd:time ;
	ex:TimeSlotEnd "10:45:00"^^xsd:time ;
	ex:DayOfWeek "Friday"^^time:dayOfWeek .

